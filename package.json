{
  "name": "@freshgum/typedi",
  "version": "0.3.24",
  "description": "Elegant Dependency Injection in JavaScript and TypeScript.",
  "homepage": "https://typedi.js.org",
  "maintainers": [
    "freshgum"
  ],
  "author": "freshgum",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "main": "./build/cjs/index.js",
  "module": "./build/esm5/index.js",
  "es2015": "./build/esm2015/index.js",
  "typings": "./build/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/freshgum-bubbles/typedi.git"
  },
  "tags": [
    "di",
    "container",
    "di-container",
    "typescript",
    "typescript-di",
    "dependency-injection",
    "typescript"
  ],
  "scripts": {
    "build": "npm run build:cjs",
    "build:clean": "rimraf build",
    "build:es2015": "tsc --project tsconfig.prod.esm2015.json",
    "build:esm5": "tsc --project tsconfig.prod.esm5.json",
    "build:cjs": "tsc --project tsconfig.prod.cjs.json",
    "build:rollup": "npm run build:esm5 && rollup --config rollup.config.mjs",
    "build:umd": "npm run build:esm5",
    "build:types": "tsc --project tsconfig.prod.types.json",
    "docs:website": "pushd website && npm run build && popd",
    "prettier:fix": "prettier --write \"**/*.{ts,md}\"",
    "prettier:check": "prettier --check \"**/*.{ts,md}\"",
    "lint:fix": "eslint --max-warnings 0 --fix --ext .ts src/",
    "lint:check": "eslint --max-warnings 0 --ext .ts src/",
    "docs:api-reference": "rimraf website/build/api-reference && typedoc",
    "test": "jest",
    "test:coverage": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --coverage --verbose",
    "prepare": "husky install",
    "prepublishOnly": "npm test && npm run build:cjs && npm run build:umd && npm run build:types",
    "website:start": "pushd website && npm run start && popd",
    "website:build": "pushd website && npm run build && popd",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "pnpm exec commitlint --edit ${1}"
    }
  },
  "lint-staged": {
    "*.md": [
      "npm run prettier:fix"
    ],
    "*.ts": [
      "npm run prettier:fix"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-angular": "^17.7.0",
    "@mxssfd/typedoc-theme": "^1.1.2",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/deepmerge": "^2.2.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "auto-changelog": "^2.4.0",
    "deepmerge": "^4.3.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^14.0.0",
    "prettier": "^2.8.8",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rollup": "^3.28.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "type-fest": "^4.2.0",
    "typedoc": "^0.24.8",
    "typedoc-github-wiki-theme": "^1.1.0",
    "typedoc-plugin-markdown": "^3.15.4",
    "typescript": "^5.1.6"
  }
}
